  "main": "main.js",
  "private": true,
  "scripts": {
    "clean": "rm -r ./dist/ | rm -r ./public/ | rm -r ./documentation/",
    "start": "webpack-dev-server --config=webpack.config.js --mode development --progress --open",
    "build-dev": "webpack --config=webpack.config.js --mode development --progress --devtool source-map",
    "build-pro": "webpack --config=webpack.config.js --mode production --progress",
    "sassdoc": "sassdoc src/scss -d ./documentation/sassdoc",
    "esdoc": "esdoc",
    "htmlhint": "htmlhint --ignore **/documentation/**, **/node_modules/**",
    "markdown": "markdown-it README.md -o README.html",
    "build-css": "node-sass ./src/scss/main.scss ./public/css/main.css",
    "build-js": "babel ./src/js/main.js -o ./public/main.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
